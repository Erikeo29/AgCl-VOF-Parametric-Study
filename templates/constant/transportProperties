FoamFile
{
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// ============================================================================
// INCLUSION DU FICHIER CENTRAL DE PARAMETRES
// ============================================================================
#include "../system/parameters"

// ============================================================================
// PROPRIETES DE TRANSPORT - PHASES
// ============================================================================

phases (water air);

water
{
    transportModel  Carreau;
    rho             $rho_ink;

    Carreau
    {
        nu0         $nu_0;          // [m²/s] Viscosite cinematique zero-shear
        nuInf       $nu_inf;        // [m²/s] Viscosite cinematique infinite-shear
        lambda      $k_carreau;     // [s] Temps de relaxation
        n           $n_carreau;     // [-] Exposant loi puissance
    }
}

air
{
    transportModel  Newtonian;
    rho             $rho_air;
    nu              $nu_air;        // [m²/s] Viscosite cinematique air
}

sigma           $sigma;             // [N/m] Tension de surface

// ************************************************************************* //
