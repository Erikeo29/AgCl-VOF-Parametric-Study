FoamFile
{
    format      ascii;
    class       dictionary;
    object      topoSetDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

// Gap 60µm - buse remontée de 30µm
// Puit: 0.8 mm x 0.128 mm
// Buse: 0.3 mm x 0.19 mm, bas à y=0.188mm (gap 60µm)
//
// Ink nozzle region: X ∈ [-0.15, 0.15] mm, Y ∈ [0.188, 0.378] mm
// In meters: X ∈ [-0.00015, 0.00015], Y ∈ [0.000188, 0.000378]

actions
(
    // Create cellSet for ink nozzle region (buse centrée)
    {
        name    inkRegionSet;
        type    cellSet;
        action  new;
        source  boxToCell;
        box     (-0.00015 0.000188 -1) (0.00015 0.000378 1);
        // X: -0.15 to 0.15 mm (nozzle width = 0.3 mm, centered)
        // Y: 0.188-0.378 mm (nozzle height = 0.19 mm, gap 60µm)
        // Z: full range
    }

    // Convert cellSet to cellZone for fvOptions
    {
        name    inkRegion;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        set     inkRegionSet;
    }
);

// ************************************************************************* //
